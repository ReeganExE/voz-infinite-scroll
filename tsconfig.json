{
  "compilerOptions": {
      "module": "commonjs",
      "noImplicitAny": false,
      "removeComments": true,
      "noUnusedLocals": true,
      "preserveConstEnums": true,
      "allowJs": true,
      "sourceMap": false,
      "outDir": "dist",
      "esModuleInterop": true,
      "target": "es2020",
      "jsx": "react-jsx"
  },
  "include": [
      "src/**/*"
  ],
  "exclude": [
      "node_modules"
  ]
}
